# Server
PORT=5005
NODE_ENV=development

# Database (Neon DB)
DATABASE_URL="postgresql://user:password@host/database?sslmode=require"

# Cloudinary
CLOUDINARY_URL="cloudinary://api_key:api_secret@cloud_name"

# Security
JWT_SECRET="your-secure-random-string-here"

